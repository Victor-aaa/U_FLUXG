		rlc.b	R14
                dec	R11
		jnz	loop_x_c

xtet_conv:
		cmp	R10, R8
		jne     npnp
		mov.b   #'.', R9
	        call	#SUMB_TO_LCD
	npnp:
		mov	#BITC, R9
	loopRla:	
		rla	R12
		rlc	R13
		rlc.b	R14
		rlc	R9
		jnc	loopRla
                jnz     convChar        // 0?
                tst.b   R5              // cut zero? high R5 = 0
                jnz     convChar        // no
		cmp	R10, R7         // end zero cut?
                jlo     nxtSmb
       convChar:
                bis     #0x00FF, R5     // cut zero off
		add.b	#'0', R9        // convert BIN to CHAR
		cmp.b	#':', R9
		jlo     wrSmb
		add.b	#'A'-':', R9
	wrSmb:	
	        call	#SUMB_TO_LCD
        nxtSmb:
		dec	R10
		jnz	xtet_conv
		swpb	R5		// restore R5
		ret
		
