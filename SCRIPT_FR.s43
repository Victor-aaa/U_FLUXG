                mov     R8, R7
                inc     R7

		swpb	R5			// N digit to LCD
		tst.b	R5
		jz	xtet_conv

		mov.b	R10, R11
		sub.b	R5, R11
		jz	xtet_conv
		jlo	xtet_conv
		sub.b	R11, R10
		rla.b	R11
		rla.b	R11		// *4
   loop_x_c:	
		rla	R12
